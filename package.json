{
	"name": "oslash-clone",
	"version": "1.0.0",
	"description": "OSlash Clone",
	"main": "dist/index.js",
	"scripts": {
		"build": "rimraf dist && tsc",
		"serve": "cross-env NODE_ENV=development ts-node-dev --respawn --pretty --transpile-only src/index.ts",
		"prestart": "npm run build",
